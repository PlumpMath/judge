<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Problem Information</title>
    <!--<script type="text/javascript" src="/static/js/tools.js"></script>-->
    <script
		  src="https://code.jquery.com/jquery-3.1.1.min.js"
		  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
		  crossorigin="anonymous">
    </script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script src="https://cdn.jsdelivr.net/clipboard.js/1.5.16/clipboard.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/static/css/judge-result.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
    integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
    crossorigin="anonymous">
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"
      integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
      crossorigin="anonymous">
    </script>
    <script src="/static/js/judge-result.js"></script>
  </head>
  <body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Judge</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li class="active"><a href="/problems">Problems</a></li>
            <li><a href="/rank">User Rank</a></li>
            <li><a href="/contest">Contests</a></li>
          </ul>
          <%- include topbar-right.ejs %>
        </div>
      </div>
    </nav>
    <div class="container" id="div-content">
      <div class="row">
        <div class="page-header">
          <h1 id="title">
            <button type="button" class="btn btn-default btn-md" href=<%= "/problems/"+id %>>Back</button>
            <%= "Stats for Problem "+id %></h1>
        </div>
      </div>
      <div class="row">
        <table class="table">
          <thead>
            <tr>
              <th>#</th>
              <th>Problem #</th>
              <th>Name</th>
              <th>Language</th>
              <th>Used Time</th>
              <th>Used Memory</th>
              <th>Result</th>
              <th>Submit Time</th>
            </tr>
          </thead>
          <tbody>
            <% var messages=['Queued','Compiling','Judging','Compile Error','Runtime Error','Memory Exceeded','Wrong Answer','Time Limit Exceeded','Output Limit Exceeded','System Error','Accepted'],
                   class_types=['label-default','label-primary','label-primary','label-danger','label-danger','label-danger','label-danger','label-danger','label-danger','label-warning','label-success']%>
            <% for(var i=0;i<submits.length;i++) { %>
              <tr>
                <td><%=submits[i].submit_id%></td>
                <td><%=submits[i].problem_id%></td>
                <td><%=submits[i].submit_user_name%></td>
                <td><%=submits[i].lang%></td>
                <td><%=submits[i].used_time%></td>
                <td><%=submits[i].used_memory%></td>
                <td class="label-result align-middle"><span class="<%="label "+class_types[submits[i].result] %>"><%= messages[submits[i].result] %></span></td>
                <td><%=submits[i].submit_time%></td>
              </tr>
            <% } %>
          </tbody>
      </div>
    </div>
  </body>
</html>
